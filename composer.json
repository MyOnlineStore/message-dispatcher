{
    "name": "myonlinestore/message-dispatcher",
    "type": "library",
    "description": "MyOnlineStore Message Dispatcher",
    "homepage": "https://github.com/MyOnlineStore/message-dispatcher",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MyOnlineStore\\MessageDispatcher\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MyOnlineStore\\MessageDispatcher\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "symfony/event-dispatcher": "^5.4 || ^6.0",
        "symfony/http-kernel": "^5.4 || ^6.0",
        "symfony/messenger": "^5.4 || ^6.0"
    },
    "require-dev": {
        "myonlinestore/coding-standard": "^3.1",
        "myonlinestore/php-devtools": "^0.2.3",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-phpunit": "^0.16",
        "vimeo/psalm": "^4.22"
    }
}
